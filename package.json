{
  "name": "mdn-checker",
  "description": "Check MDN page structures",
  "version": "0.0.0",
  "author": "Joshua Chen <sidachen2003@gmail.com>",
  "license": "MIT",
  "type": "module",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jc-verse/mdn-checker.git"
  },
  "bugs": {
    "url": "https://github.com/jc-verse/mdn-checker/issues"
  },
  "homepage": "https://jc-verse.github.io/mdn-checker/",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "run:check": "node ./dist/index.js",
    "format": "prettier -w .",
    "lint": "eslint \"**/*.{js,ts,jsx,tsx}\"",
    "spellcheck": "cspell \"**\" --no-progress",
    "prepare": "husky install"
  },
  "packageManager": "yarn@3.5.0",
  "dependencies": {
    "core-js": "^3.30.0",
    "front-matter": "^4.0.2",
    "mdast-util-to-string": "^3.2.0",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.1",
    "unified": "^10.1.2",
    "unist-util-visit": "^4.1.2",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "cspell": "^6.31.1",
    "eslint": "^8.37.0",
    "eslint-config-jc": "^2.3.3",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "^2.27.5",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "prettier": "^2.8.7",
    "prettier-config-jc": "^2.3.0",
    "tsconfig-jc": "^2.3.0",
    "typescript": "^5.0.3"
  },
  "resolutions": {
    "@types/mdast@^3.0.0": "patch:@types/mdast@npm%3A3.0.11#./.yarn/patches/@types-mdast-npm-3.0.11-4765a259eb.patch"
  }
}
